{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "interface-name": [false, "never-prefix"],
        "object-literal-sort-keys": false,
        "max-line-length": [true, 140],
        "ordered-imports": false,
        "no-console": false
    },
    "rulesDirectory": []
}
